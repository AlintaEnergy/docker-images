<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/"
    xmlns:set="http://www.sitecore.net/xmlconfig/set/">
    <sitecore>
        <pipelines>
            <initialize>
                <processor type="Sitecore.Pipelines.Loader.ShowVersion, Sitecore.Kernel">
                    <patch:delete />
                </processor>
            </initialize>
        </pipelines>
        <log4net>
            <appender name="LogFileAppender" patch:instead="*[@name='LogFileAppender']" set:type="log4net.Appender.UdpAppender, Sitecore.Logging">
                <remoteAddress value="127.0.0.1" />
                <remotePort value="7777" />
                <encoding value="utf-8" />
                <layout type="log4net.Layout.PatternLayout">
                    <conversionPattern value="system       %d{yyyy-MM-dd HH:mm:ss.fff} %-5p (%4t) %m" />
                </layout>
            </appender>
            <appender name="PublishingLogFileAppender" patch:instead="*[@name='PublishingLogFileAppender']" set:type="log4net.Appender.UdpAppender, Sitecore.Logging">
