                <remotePort value="7777" />
                <encoding value="utf-8" />
                <layout type="log4net.Layout.PatternLayout">
                    <conversionPattern value="publishing   %d{yyyy-MM-dd HH:mm:ss.fff} %-5p (%4t) %m"/>
                </layout>
            </appender>
            <appender name="ClientLogAppender" patch:instead="*[@name='ClientLogAppender']" set:type="log4net.Appender.UdpAppender, Sitecore.Logging">
